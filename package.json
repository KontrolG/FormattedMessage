{
	"name": "formatted-message",
	"displayName": "FormattedMessage",
	"repository": "https://github.com/etaiklein/FormattedMessage",
	"publisher": "etaiklein",
	"description": "Replaces a string with <FormattedMessage /> syntax to make translating strings using react-intl easier",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.formattedMessage"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.formattedMessage",
				"title": "FormattedMessage (javascript)"
			},
			{
				"command": "extension.transMessage",
				"title": "trans.message (python)"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.formattedMessage",
					"group": "navigation"
				},
				{
					"command": "extension.transMessage",
					"group": "navigation"
				}
			]
		},
		"keybindings": [
			{
				"command": "extension.formattedMessage",
				"key": "ctrl+shift+t",
				"mac": "cmd+shift+t"
			},
			{
				"command": "extension.transMessage",
				"key": "ctrl+shift+r",
				"mac": "cmd+shift+r"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"keywords": [
		"translation",
		"FormattedMessage",
		"i18n",
		"react-intl",
		"translate"
	],
	"devDependencies": {},
	"dependencies": {}
}
